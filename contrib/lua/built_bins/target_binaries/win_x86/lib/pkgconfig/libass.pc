prefix=/usr/win32
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: libass
Description: LibASS is an SSA/ASS subtitles rendering library
Version: 0.9.8
Requires.private: fontconfig >= 2.2.0, freetype2 >= 9.6.3
Libs: -L${libdir} -lass
Libs.private: -lm
Cflags: -I${includedir}
